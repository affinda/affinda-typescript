    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "dist-esm/**/*.js",
    "dist-esm/**/*.js.map",
    "dist-esm/**/*.d.ts",
    "dist-esm/**/*.d.ts.map",
    "src/**/*.ts",
    "README.md",
